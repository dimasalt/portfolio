@attribute [StreamRendering(true)]

<div class="vg-page page-about" id="about">
    <div class="container py-5">
        <div class="row">
            <div class="col-lg-4 py-3">
                <div class="img-place wow fadeInUp">
                    <img src="img/me.png" alt="">
                </div>
            </div>
            <div class="col-lg-6 offset-lg-1 mb-3 wow fadeInRight">
                <h1 class="fw-light pt-4 pt-md-0">Dmitri Saltanovich</h1>
                <h5 class="fg-theme mb-3">Software Engineer</h5>
                <p class="text-muted">
                    My name is Dmitri and I am Software Engineer with over 12 years of commercial professional experience. With main focus 
                    on .NET/MVC.NET/Blazor and React/Next.js 
                </p>
                <ul class="theme-list">
                    <li>Over 12 years of experience in software industry primarly as an Application Developer</li>
                    <li>Expertise in C#, .NET, Javascript, ReactJS, Next.js, TypeScript, Microsoft SQL Server and MySQL, Docker</li>
                    <li>Strong experience with .NET MVC, Blazor, Bootstrap and TailwindCSS, JQuery, AngularJS, VueJS, KnockoutJS</li>
                    <li>Done programming in Java and PHP in the past.</li>
                    <li>Strong debugging skills</li>
                    <li>Great communication skills and ability to work in teams on large scale projects.</li>
                </ul>
                <button class="btn btn-theme-outline">Download CV</button>
                <button class="btn btn-theme-outline">Download Cover Letter</button>
            </div>
        </div>
    </div>
    <div class="container py-5">
        <h1 class="text-center fw-normal wow fadeIn">My Skills</h1>
        <div class="row py-3">
            <div class="col-md-6">
                <div class="px-lg-3">
                    <h4 class="wow fadeInUp">Coding skills</h4>
                    <div class="progress-wrapper wow fadeInUp">
                        <span class="caption">.NET</span>
                        <div class="progress">
                            <div class="progress-bar" role="progressbar" style="width: 100%;" aria-valuenow="75" aria-valuemin="0" aria-valuemax="100">100%</div>
                        </div>
                    </div>
                    <div class="progress-wrapper wow fadeInUp">
                        <span class="caption">Javascript/Typescript</span>
                        <div class="progress">
                            <div class="progress-bar" role="progressbar" style="width: 100%;" aria-valuenow="75" aria-valuemin="0" aria-valuemax="100">100%</div>
                        </div>
                    </div>
                    <div class="progress-wrapper wow fadeInUp">
                        <span class="caption">React/Next.js/Express.js</span>
                        <div class="progress">
                            <div class="progress-bar" role="progressbar" style="width: 100%;" aria-valuenow="75" aria-valuemin="0" aria-valuemax="100">100%</div>
                        </div>
                    </div>
                    <div class="progress-wrapper wow fadeInUp">
                        <span class="caption">PHP</span>
                        <div class="progress">
                            <div class="progress-bar" role="progressbar" style="width: 80%;" aria-valuenow="75" aria-valuemin="0" aria-valuemax="100">80%</div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-md-6">
                <div class="px-lg-3">
                    <h4 class="wow fadeInUp">Design Skills</h4>
                    <div class="progress-wrapper wow fadeInUp">
                        <span class="caption">MVC.NET/Blazor</span>
                        <div class="progress">
                            <div class="progress-bar" role="progressbar" style="width: 100%;" aria-valuenow="75" aria-valuemin="0" aria-valuemax="100">100%</div>
                        </div>
                    </div>
                    <div class="progress-wrapper wow fadeInUp">
                        <span class="caption">HTML + CSS</span>
                        <div class="progress">
                            <div class="progress-bar" role="progressbar" style="width: 100%;" aria-valuenow="75" aria-valuemin="0" aria-valuemax="100">92%</div>
                        </div>
                    </div>
                    <div class="progress-wrapper wow fadeInUp">
                        <span class="caption">Bootstrap</span>
                        <div class="progress">
                            <div class="progress-bar" role="progressbar" style="width: 100%;" aria-valuenow="75" aria-valuemin="0" aria-valuemax="100">100%</div>
                        </div>
                    </div>
                    <div class="progress-wrapper wow fadeInUp">
                        <span class="caption">TailwindCSS</span>
                        <div class="progress">
                            <div class="progress-bar" role="progressbar" style="width: 95%;" aria-valuenow="75" aria-valuemin="0" aria-valuemax="100">95%</div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="container pt-5">
        <div class="row">
            <div class="col-md-6 wow fadeInRight">
                <h2 class="fw-normal">Education</h2>
                <ul class="timeline mt-4 pr-md-5">
                    <li>
                        <div class="title">2009</div>
                        <div class="details">
                            <h5>Advanced Web Development for e-Business Certificate</h5>
                            <small class="fg-theme">Humber College Institute of Technology & Advanced Learning</small>
